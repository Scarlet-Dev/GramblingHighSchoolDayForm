  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-nodemailer": "^1.1.3",
    "faker": "^4.1.0",
    "knex": "^0.21.1",
